#ifndef SEGMENTOS_H
#define	SEGMENTOS_H

#define disp0 0x3F ;00111111
#define disp1 0x06 ;00000110
#define disp2 0x5B ;01011011
#define disp3 0x4F ;01001111
#define disp4 0x66 ;01100110
#define disp5 0x6D ;01101101
#define disp6 0x7D ;01111101
#define disp7 0x07 ;00000111
#define disp8 0x7F ;01111111
#define disp9 0x6F ;01101111
#define dispA 0x77 ;01110111
#define dispB 0x7C ;01111100
#define dispC 0x39 ;00111001
#define dispD 0x5E ;01011110
#define dispE 0x79 ;01111001
#define dispF 0x71 ;01110001

#endif	
